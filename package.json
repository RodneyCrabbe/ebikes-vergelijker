{
  "name": "ebike-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node scripts/check-env.js && vite --host 127.0.0.1 --port 5195",
    "dev:check": "node scripts/check-env.js",
    "dev:frontend": "vite --host 127.0.0.1 --port 5195",
    "dev:supabase": "npx supabase start",
    "dev:clean": "npm run clean && npm run dev",
    "dev:persistent": "pm2 start ecosystem.config.js",
    "dev:stop": "pm2 stop all",
    "dev:restart": "pm2 restart all",
    "dev:monitor": "pm2 monit",
    "dev:logs": "pm2 logs",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:status": "npx supabase status",
    "supabase:reset": "npx supabase db reset",
    "build": "node scripts/check-env.js && vue-tsc && vite build",
    "preview": "vite preview",
    "start": "npm run dev",
    "clean": "taskkill /F /IM node.exe 2>nul || echo No node processes",
    "full-restart": "npm run clean && npm run supabase:stop && timeout /t 3 && npm run supabase:start && timeout /t 5 && npm run dev",
    "health": "node scripts/health-check.js",
    "setup": "node scripts/check-env.js && npm run supabase:status",
    "health-check": "node scripts/check-env.js && echo 'Environment check complete'",
    "auth:test": "node scripts/fix-auth-persistence.js test",
    "auth:fix": "node scripts/fix-auth-persistence.js fix",
    "auth:backup": "powershell -ExecutionPolicy Bypass -File scripts/persist-auth-data.ps1 -Action backup",
    "auth:restore": "powershell -ExecutionPolicy Bypass -File scripts/persist-auth-data.ps1 -Action restore",
    "auth:status": "powershell -ExecutionPolicy Bypass -File scripts/persist-auth-data.ps1 -Action status",
    "supabase:restart": "npm run supabase:stop && timeout /t 3 && npm run supabase:start",
    "test": "vitest",
    "test:unit": "vitest run src/test/unit",
    "test:integration": "vitest run src/test/integration",
    "test:performance": "vitest run src/test/performance",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:full": "node tests/e2e/full-system-test.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance && npm run test:e2e",
    "test:pages": "node scripts/test-all-pages.js",
    "debug:errors": "node scripts/debug-errors.js",
    "test:navigation": "node scripts/test-navigation.js",
    "test:router": "node scripts/test-router.js",
    "test:direct": "node scripts/test-direct-navigation.js",
    "test:links": "node scripts/test-link-clicks.js",
    "db:backup": "powershell -ExecutionPolicy Bypass -File ./scripts/backup-local-db.ps1",
    "db:restore": "powershell -ExecutionPolicy Bypass -File ./scripts/restore-local-db.ps1",
    "db:status": "npx supabase status",
    "db:restart": "npx supabase stop && npx supabase start"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@supabase/supabase-js": "^2.74.0",
    "@tanstack/vue-virtual": "^3.13.14",
    "chart.js": "^4.5.0",
    "dayjs": "^1.11.18",
    "lodash-es": "^4.17.21",
    "nuqs": "^2.7.1",
    "pinia": "^3.0.3",
    "terser": "^5.44.0",
    "vite": "^7.1.9",
    "vue": "^3.5.22",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@storybook/vue3": "^9.1.10",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^3.2.4",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "puppeteer": "^24.24.0",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  }
}
